{
  "name": "fullstack-effect",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"bun --watch src/server/main.ts\"",
    "dev:client": "vite",
    "dev:server": "bun --watch src/server/main.ts",
    "build": "tsc -b && vite build",
    "test": "bunx vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "preview": "vite preview",
    "prepare": "effect-language-service patch"
  },
  "dependencies": {
    "@effect-atom/atom-react": "^0.3.4",
    "@effect/platform": "^0.92.1",
    "@effect/platform-browser": "^0.72.0",
    "@effect/platform-bun": "^0.81.1",
    "@effect/rpc": "^0.71.1",
    "@tailwindcss/vite": "^4.1.16",
    "effect": "^3.18.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@effect/language-service": "^0.51.0",
    "@effect/vitest": "^0.26.0",
    "@types/bun": "^1.3.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "typescript": "~5.9.3",
    "vite": "^7.1.12"
  }
}
